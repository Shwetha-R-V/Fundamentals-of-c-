#include<iomanip>
using namespace std;
int main(){
    int a,b,c;
    float d;
    cin>>a>>b>>c>>d;
    cout<<(a*a);
    float e=(3.14*d*d);
    cout<<"\n"<<(b*c);
    cout<<"\n"<<fixed<<setprecision(2)<<e;
    
}
