//Write a C program to print the sample output.
Sample Output :
Hello World...!
program:
#include<iostream>
using namespace std;
int main()
{
    cout<<"Hello World...!";
    return 0;
}
//Write a C program to print sample output.
Sample Output :
Hello World...!
Hello World...!
program:
#include<iostream>
using namespace std;
int main()
{
    cout<<"Hello World...!";
    cout<<"\nHello World...!";
}
//Write a C program to print sample Output.
Sample Output:
Hello     World...!
program:
#include<iostream>
using namespace std;
int main()
{
    cout<<"Hello\tWorld...!";
    return 0;
}
//Input consists of 1 character. Output consists of 1 character.
